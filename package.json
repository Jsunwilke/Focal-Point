    "photography",
    "sports",
    "studio-management"
  ],
  "main": "src/index.js",
  "homepage": "/",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@react-google-maps/api": "^2.20.7",
    "bcryptjs": "^3.0.2",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.11.0",
    "chart.js": "^4.4.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "file-saver": "^2.0.5",
    "firebase": "^10.12.0",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.2.0",
    "lucide-react": "^0.400.0",
    "prisma": "^6.13.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.9.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "env-cmd": "^10.1.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "deploy": "npm run build:prod && firebase deploy --only hosting",
    "deploy:preview": "npm run build:prod && firebase deploy --only hosting --project preview"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
