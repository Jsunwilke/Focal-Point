# Firebase Configuration
# Copy this file to .env.local and fill in your actual Firebase project values
# DO NOT commit .env.local to version control

# Firebase API Key
REACT_APP_FIREBASE_API_KEY=your_api_key_here

# Firebase Auth Domain (usually your-project-id.firebaseapp.com)
REACT_APP_FIREBASE_AUTH_DOMAIN=your_auth_domain_here

# Firebase Project ID
REACT_APP_FIREBASE_PROJECT_ID=your_project_id_here

# Firebase Storage Bucket (usually your-project-id.firebasestorage.app)
REACT_APP_FIREBASE_STORAGE_BUCKET=your_storage_bucket_here

# Firebase Messaging Sender ID
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id_here

# Firebase App ID
REACT_APP_FIREBASE_APP_ID=your_app_id_here

# Firebase Measurement ID (for Google Analytics)
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Development Environment Settings
NODE_ENV=development